
/*tab*/
QTabWidget#tabWidget_main::tab-bar
{
    border-image:url(:/imgs/main_wnd_bg.png);
}

QWidget#widget_main
{
    border-top:1px solid rgb(212, 221, 236);
    background-color:  rgb(64, 111, 191);
}

QWidget#widget_img
{
    background-color: rgb(255, 255, 255);
}

/*十字*/
QPushButton#pushButton_cross
{
        border-image:url(:/imgs/scale_normal.png);
}

QPushButton#pushButton_cross:hover
{
        border-image:url(:/imgs/scale_hover.png);
}

QPushButton#pushButton_cross:checked
{
        border-image:url(:/imgs/scale_active.png);
}

/*滑块*/
QSlider#verticalSlider_diameter:vertical {
    width: 18px;
    border-width: 12px 0px 12px 0px;
    border-image:url(:/imgs/Big_scrollbar_bg.png)12 0 12 0 repeat repeat;
}

QSlider#verticalSlider_diameter::groove:vertical
{
    subcontrol-origin:content;
    width:18px;
    background:transparent;
}

QSlider#verticalSlider_diameter::handle:vertical
{
    margin-top:-12px;
    margin-bottom:-12px;
    image:url(:/imgs/Big_scrollbar_chunk_normal.png);
}

QSlider#verticalSlider_diameter::handle:hover:vertical
{
    image:url(:/imgs/Big_scrollbar_chunk_hover.png);
}

QSlider#verticalSlider_diameter::handle:pressed:vertical
{
    image:url(:/imgs/Big_scrollbar_chunk_active.png);
}

QSlider::add-page:vertical {
    background:transparent;
 }

/*滑块滑后*/
 QSlider::sub-page:vertical {
    background:transparent;
 }

