QScrollBar:horizontal  {
    height: 17px;
    border-width:0px 8px 0px 8px;
    border-image: url(:/imgs/m_scrollbar_bg.png) 0 8 0 8 repeat repeat;
        padding-right:0px;
        padding-left:0px;
}

QScrollBar::handle:horizontal  {
   min-width:17px;
   border-width: 0px 8px 0px 8px;
   border-image: url(:/imgs/m_scrollbar_chunk.png)0 8 0 8 repeat repeat;
        margin-left:9px;
        margin-right:9px;
}

QScrollBar::handle:hover:horizontal  {
    border-image: url(:/imgs/m_scrollbar_chunk.png)0 8 0 8 repeat repeat;
}

QScrollBar::handle:pressed:horizontal  {
    border-image: url(:/imgs/m_scrollbar_chunk.png)0 8 0 8 repeat repeat;
}

QScrollBar::handle:!enabled:horizontal  {
    border-image: url(:/imgs/m_scrollbar_chunk.png)0 8 0 8 repeat repeat;
}

QScrollBar::add-page:horizontal
{
    border:none;
    background:transparent;
}

QScrollBar::sub-page:horizontal
{
    border:none;
    background:transparent;
}

QScrollBar::sub-line:horizontal  {
   width: 16px;
   height: 17px;

   subcontrol-position: left;
   subcontrol-origin: margin;
        left:1px;
   background-image: url(:/imgs/m_scrollbar_Left_normal.png);
}

QScrollBar::sub-line:hover:horizontal{
   background-image: url(:/imgs/m_scrollbar_Left_hover.png);
}

QScrollBar::sub-line:pressed:horizontal{
   background-image: url(:/imgs/m_scrollbar_Left_active.png);
}

QScrollBar::sub-line:!enabled:horizontal{
   background-image: url(:/imgs/m_scrollbar_Left_normal.png);
}

QScrollBar::add-line:horizontal  {
    width: 16px;
    height: 17px;
        right:1px;
    subcontrol-position: right;
    subcontrol-origin: margin;
    background-image: url(:/imgs/m_scrollbar_right_normal.png);
}

QScrollBar::add-line:hover:horizontal{
   background-image: url(:/imgs/m_scrollbar_right_hover.png);
}

QScrollBar::add-line:pressed:horizontal{
   background-image: url(:/imgs/m_scrollbar_right_active.png);
}

QScrollBar::add-line:!enabled:horizontal{
   background-image: url(:/imgs/m_scrollbar_right_normal.png);
}

QScrollBar:vertical  {
    width: 17px;
    border-width:8px 0px 8px 0px;
    border-image: url(:/imgs/m_vscrollbar_bg.png) 8 0 8 0 repeat repeat;
        margin-top: 0px;
}

QScrollBar::handle:vertical  {
   min-height:17px;
   border-width: 8px 0px 8px 0px;
   border-image: url(:/imgs/m_vscrollbar_chunk.png) 8 0 8 0 repeat repeat;
        margin-top:22px;
        margin-bottom:9px;
}

QScrollBar::handle:hover:vertical  {
    border-image: url(:/imgs/m_vscrollbar_chunk.png) 8 0 8 0 repeat repeat;
}

QScrollBar::handle:pressed:vertical  {
    border-image: url(:/imgs/m_vscrollbar_chunk.png) 8 0 8 0 repeat repeat;
}

QScrollBar::handle:!enabled:vertical  {
    border-image: url(:/imgs/m_vscrollbar_chunk.png) 8 0 8 0 repeat repeat;
}

QScrollBar::add-page:vertical
{
    border:none;
    background:transparent;
}

QScrollBar::sub-page:vertical
{
    border:none;
    background:transparent;
}

QScrollBar::sub-line:vertical{
   height: 16px;
   width: 17px;
   subcontrol-position:up;
   subcontrol-origin: margin;
        top:1px;
   background-image: url(:/imgs/m_scrollbar_up_normal.png);
}

QScrollBar::sub-line:hover:vertical{
   background-image: url(:/imgs/m_scrollbar_up_hover.png);
}

QScrollBar::sub-line:pressed:vertical{
   background-image: url(:/imgs/m_scrollbar_up_active.png);
}

QScrollBar::sub-line:!enabled:vertical{
   background-image: url(:/imgs/m_scrollbar_up_normal.png);
}

QScrollBar::add-line:vertical{
    height: 16px;
    width: 17px;
        bottom:1px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
    background-image: url(:/imgs/m_scrollbar_down_normal.png);
}

QScrollBar::add-line:hover:vertical{
   background-image: url(:/imgs/m_scrollbar_down_hover.png);
}

QScrollBar::add-line:pressed:vertical{
   background-image: url(:/imgs/m_scrollbar_down_active.png);
}

QScrollBar::add-line:!enabled:vertical{
   background-image: url(:/imgs/m_scrollbar_down_normal.png);
}
